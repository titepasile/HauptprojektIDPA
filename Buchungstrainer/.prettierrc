{
    "singleQuote": false,
    "trailingComma": "none",
    "bracketSpacing": true,
    "tabWidth": 4,
    "printWidth": 100,
    "semi": true,
    "endOfLine": "lf",
    "svelteSortOrder": "scripts-markup-styles-options",
    "plugins": ["prettier-plugin-svelte"],
    "pluginSearchDirs": ["."],
    "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
